{
  "expo": {
    "name": "logic and memory",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icons.png",
    "splash": {
      "backgroundColor": "#FFFFFF"
    },
    "androidStatusBar": {
      "hidden": true
    },
    "plugins": [
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "inset-swipe",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconsMini.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lowech.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "inset-swipe",
        "backgroundColor": "#FFFFFF"
      },
      "eas": {
        "projectId": "79e6055d-11ee-4b3e-a4b7-6dde311d0a69"
      }
    }
  }
}
